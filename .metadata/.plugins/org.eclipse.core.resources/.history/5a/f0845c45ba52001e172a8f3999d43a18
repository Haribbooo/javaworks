package kr.or.iei;

public class User {
	private String id;
	private String pwd;
	private String name;
	private int age;
	private String phone;		

public User() { }

public void setId(String id) {
	this.id=id;
}
public String getId() {
	return id;
}
public void setPwd(String pwd) {
	this.pwd=pwd;
}
public String getPwd() {
	return pwd;
}
public void setName(String name) {
	this.name=name;
}
public String getName() {
	return name;
}
public void setAge(int age) {
	this.age=age;
}
public int getAge() {
	return age;
}
public void setPhone(String phone) {
	this.phone=phone;
}
public String getPhone() {
	return phone;
}



}
