package arrarys;

public class ArrayGuugudan {

	public static void main(String[] args) {

			//구구단 3단 출력 배열로 안하는거 
		
		 int dan =3 ;
		 
		 for(int i = 1; i<10;i++) {
			 System.out.println(dan + " X "+ "=" + dan*i);			 
		 }
		 System.out.println("=================");
		 
		 //배열을 사용해서 구구단 저장 및 출력
		 int[] gugudan = new int[9];
		 
		 for(int i = 0; i<gugudan.length+1; i++) {
			 System.out.println("3 x " + (i+1) + "=" + 3*(i+1));
		 }

	}

}
