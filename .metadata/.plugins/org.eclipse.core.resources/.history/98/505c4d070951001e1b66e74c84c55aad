package kr.or.iei.model;

public class User {
	private String id;
	private String pw;
	private String name;
	
	//set 필드이름 () ,get 필드이름 ()함수
	
	public void setId(String id) {
		this.id = id;		
	}
	public String getId() {
		return id;
	}
	public void setPw(String pw) {
		this.pw = pw;
	}
	public String getPw() {
		return pw;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getName() {
		return name;
	}
	
	
	
}
